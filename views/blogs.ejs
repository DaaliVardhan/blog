

<section class="blog-list px-3 py-5 p-md-5 mt-5">
    <div class="container">




<% if (blogs) blogs.forEach(blog => { %> 

    <div class="item mb-5">
        <div class="media">
            <img class="mr-3 img-fluid post-thumb d-none d-md-flex" src="<%- "/assets/images/blog/blog-post-thumb-5.jpg" || blog.thumbnail   %>" alt="image">
            <div class="media-body">
                <h3 class="title mb-1"><a href="<%- blog.slug%>"><%- blog.title %></a></h3>
                <div class="meta mb-1"><span class="date">Published on <%- blog.publishedOn.toLocaleDateString()%></span><span class="time">5 min read</span><span class="comment"><i  class="fa fa-thumbs-up" ></i></span><span id="likes"><%- blog.votes || 0 %> likes</span></div>
                <div class="intro"><%- blog.description%>...</div>
                <a class="more-link" href="<%- blog.slug%>">Read more &rarr;</a>
            </div>
        </div>
    </div>

    <% }) %>



    <nav class="blog-nav nav nav-justified my-5">
        <a class="nav-link-prev nav-item nav-link d-none rounded-left" href="/">Previous<i class="arrow-prev fas fa-long-arrow-alt-left"></i></a>
        <a class="nav-link-next nav-item nav-link rounded" href="/blogs">Next<i class="arrow-next fas fa-long-arrow-alt-right"></i></a>
      </nav>
      
  </div>
</section>